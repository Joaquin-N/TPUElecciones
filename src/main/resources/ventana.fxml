<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaz.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane prefHeight="500.0" prefWidth="600.0">
            <children>
                <Button fx:id="btnDirectorio" layoutX="493.0" layoutY="58.0" mnemonicParsing="false" onAction="#btnDirectorio_OnPressed" prefHeight="25.0" prefWidth="71.0" text="Cambiar" />
                <Label layoutX="40.0" layoutY="37.0" text="Directorio de búsqueda de archivos" />
                <TextField fx:id="txtDirectorio" disable="true" layoutX="40.0" layoutY="58.0" prefHeight="25.0" prefWidth="440.0" />
                <ComboBox fx:id="cmbDistrito" layoutX="93.0" layoutY="177.0" onAction="#cmbDistrito_SelectionChanged" prefHeight="25.0" prefWidth="188.0" />
                <ComboBox fx:id="cmbSeccion" layoutX="93.0" layoutY="226.0" onAction="#cmbSeccion_SelectionChanged" prefHeight="25.0" prefWidth="188.0" />
                <ComboBox fx:id="cmbCircuito" layoutX="93.0" layoutY="272.0" onAction="#cmbCircuito_SelectionChanged" prefHeight="25.0" prefWidth="188.0" />
                <ComboBox fx:id="cmbMesa" layoutX="93.0" layoutY="316.0" prefHeight="25.0" prefWidth="188.0" />
                <Label layoutX="31.0" layoutY="181.0" text="Distrito" />
                <Label layoutX="30.0" layoutY="230.0" text="Sección" />
                <Label layoutX="30.0" layoutY="276.0" text="Circuito" />
                <Label layoutX="41.0" layoutY="320.0" text="Mesa" />
                <Button fx:id="btnFiltrar" layoutX="71.0" layoutY="390.0" mnemonicParsing="false" onAction="#btnFiltrar_OnPressed" prefHeight="25.0" prefWidth="81.0" text="Filtrar" />
                <TableView fx:id="tblConteos" layoutX="321.0" layoutY="116.0" prefHeight="351.0" prefWidth="243.0">
                    <columns>
                        <TableColumn fx:id="colAgrupacion" prefWidth="171.0" text="Agrupación" />
                        <TableColumn fx:id="colVotos" prefWidth="71.0" text="Votos" />
                    </columns>
                </TableView>
                <Button fx:id="btnSalir" layoutX="180.0" layoutY="390.0" mnemonicParsing="false" onAction="#btnSalir_OnPressed" prefHeight="25.0" prefWidth="81.0" text="Salir" />
            </children></Pane>
    </children>
</GridPane>
