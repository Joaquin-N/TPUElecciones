<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaz.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="pane" prefHeight="470.0" prefWidth="840.0">
            <children>
                <Button fx:id="btnDirectorio" layoutX="355.0" layoutY="101.0" mnemonicParsing="false" onAction="#btnDirectorio_OnPressed" prefHeight="25.0" prefWidth="71.0" text="Cambiar" />
            <Button fx:id="btnCargar" layoutX="27.0" layoutY="23.0" mnemonicParsing="false" onAction="#btnCargar_OnPressed" text="Cargar desde BD" />
                <Label layoutX="45.0" layoutY="80.0" text="Directorio de búsqueda de archivos" />
                <TextField fx:id="txtDirectorio" disable="true" layoutX="45.0" layoutY="101.0" prefHeight="25.0" prefWidth="302.0" />
                <ComboBox fx:id="cmbDistrito" layoutX="122.0" layoutY="185.0" onAction="#cmbDistrito_SelectionChanged" prefHeight="25.0" prefWidth="233.0" />
                <ComboBox fx:id="cmbSeccion" layoutX="122.0" layoutY="234.0" onAction="#cmbSeccion_SelectionChanged" prefHeight="25.0" prefWidth="233.0" />
                <ComboBox fx:id="cmbCircuito" layoutX="122.0" layoutY="280.0" onAction="#cmbCircuito_SelectionChanged" prefHeight="25.0" prefWidth="233.0" />
                <ComboBox fx:id="cmbMesa" layoutX="122.0" layoutY="324.0" prefHeight="25.0" prefWidth="233.0" />
                <Label layoutX="60.0" layoutY="189.0" text="Distrito" />
                <Label layoutX="59.0" layoutY="238.0" text="Sección" />
                <Label layoutX="59.0" layoutY="284.0" text="Circuito" />
                <Label layoutX="70.0" layoutY="328.0" text="Mesa" />
                <Button fx:id="btnFiltrar" layoutX="126.0" layoutY="415.0" mnemonicParsing="false" onAction="#btnFiltrar_OnPressed" prefHeight="25.0" prefWidth="81.0" text="Filtrar" />
            <Button fx:id="btnGuardar" layoutX="146.0" layoutY="23.0" mnemonicParsing="false" onAction="#btnGuardar_OnPressed" prefHeight="25.0" prefWidth="105.0" text="Guardar en BD" />
                <Button fx:id="btnSalir" layoutX="241.0" layoutY="415.0" mnemonicParsing="false" onAction="#btnSalir_OnPressed" prefHeight="25.0" prefWidth="81.0" text="Salir" />
                <TableView fx:id="tblConteos" layoutX="453.0" layoutY="35.0" prefHeight="413.0" prefWidth="367.0">
                    <columns>
                        <TableColumn fx:id="colAgrupacion" prefWidth="282.0" text="Agrupación" />
                        <TableColumn fx:id="colVotos" prefWidth="84.0" text="Votos" />
                    </columns>
                </TableView>
            </children></Pane>
    </children>
</GridPane>
